datasource db {
    provider = "postgresql"
    url      = env("POSTGRES_URL")
}

generator db {
    provider = "go run github.com/steebchen/prisma-client-go"
}

model User {
    id        String   @id @default(cuid())
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    fname     String
    lname     String
    username  String
    email     String   @unique
}
